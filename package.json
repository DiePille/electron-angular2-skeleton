{
  "name": "electron-angular2-skeleton",
  "version": "1.0.0",
  "description": "A electron skeleton application with angular2 support.",
  "main": "main.js",
  "scripts": {
    "postinstall": "npm run typings",
    "prestart": "npm run tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron main.js",
    "tsc": "tsc",
    "typings": "typings install",
    "dist-win32-x64": "npm run tsc && electron-packager . --out=dist --platform=win32 --arch=x64 --overwrite",
    "dist-all": "npm run tsc && electron-packager . --out=dist --platform=all --arch=all --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DonUrks/electron-angular2-skeleton.git"
  },
  "keywords": [
    "node",
    "typescript",
    "angular2",
    "electron"
  ],
  "author": "Eric MÃ¼ller",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DonUrks/electron-angular2-skeleton/issues"
  },
  "homepage": "https://github.com/DonUrks/electron-angular2-skeleton#readme",
  "dependencies": {
    "electron-packager": "^7.2.0",
    "electron-prebuilt": "^1.2.5",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",

    "@angular/common":  "2.0.0-rc.4",
    "@angular/compiler":  "2.0.0-rc.4",
    "@angular/core":  "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http":  "2.0.0-rc.4",
    "@angular/platform-browser":  "2.0.0-rc.4",
    "@angular/platform-browser-dynamic":  "2.0.0-rc.4",
    "@angular/router":  "3.0.0-beta.2",
    "@angular/router-deprecated":  "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",

    
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",

    "angular2-in-memory-web-api": "0.0.14",
    "bootstrap": "^3.3.6"
  }
}
